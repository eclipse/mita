package stdlib;

/**
 * Prints a string via USB/serial.
 */
export generated fn print(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.PrintGenerator";

/**
 * Prints a string via USB/serial appending a newline.
 */
export generated fn println(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.PrintGenerator";

/**
 * Logs a debug message using the platform logging capabilities.
 */
export generated fn logDebug(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.LogGenerator";

/**
 * Logs an info message using the platform logging capabilities.
 */
export generated fn logInfo(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.LogGenerator";
	
/**
 * Logs a warning message using the platform logging capabilities.
 */
export generated fn logWarning(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.LogGenerator";
	
/**
 * Logs an error message using the platform logging capabilities.
 */
export generated fn logError(msg : string) : void
	generator "org.eclipse.mita.library.stdlib.functions.LogGenerator";
	
